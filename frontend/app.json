{
  "expo": {
    "name": "PlayVu",
    "slug": "PlayVu",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.connerk2001.PlayVu",
      "infoPlist": {
      "UIBackgroundModes": ["location", "fetch", "remote-notification"],
      "NSLocationAlwaysAndWhenInUseUsageDescription": "App requires geolocation to improve the quality of the service",
      "NSLocationAlwaysUsageDescription": "App requires geolocation to improve the quality of the service",
      "NSLocationWhenInUseUsageDescription": "App requires geolocation to improve the quality of the service"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.connerk2001.PlayVu"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      [
        "expo-router"
      ] 
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4834b0a2-7233-4459-9f51-f2f2f6d0bc76"
      }
    }
  }
}
